prog "LXTerminal" ! /usr/bin/lxterminal 
prog "Terminal" ! /usr/bin/lxterm 
prog "Clementine" ! /usr/bin/clementine 
prog "Falkon" ! /usr/bin/falkon 
prog "Conky" !  /usr/bin/conky 

prog "Ativar Wi-Fi (root)" ! /usr/local/bin/ativar_wifi.sh
prog "Desativar Wi-Fi (root)" ! /usr/local/bin/desativar_wifi.sh

